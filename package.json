{
  "name": "react-code-view",
  "version": "1.0.0",
  "description": "- The design is for reference only, feel free to use your own one: https://marvelapp.com/4f4j6j1/screen/46362067",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch",
    "coverage": "jest --coverage --no-cache",
    "lint": "eslint src && stylelint \"src/**/*.css\"",
    "stats": "webpack-bundle-analyzer stats/stats.json",
    "start": "NODE_ENV=development webpack-dev-server --config webpack.config.dev.js",
    "build": "npm run build:production",
    "build:development": "NODE_ENV=development webpack --config webpack.config.prod.js --progress",
    "build:production": "NODE_ENV=production webpack --config webpack.config.prod.js --progress"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tuanhung9293/react-code-view.git"
  },
  "author": "tuan-ho",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "camelize": "^1.0.0",
    "classnames": "^2.2.6",
    "core-js": "^2.5.7",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-audio-player": "^0.11.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "snakeize": "^0.1.0",
    "video-react": "^0.13.1",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^9.3.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^1.0.0",
    "compression-webpack-plugin": "^1.1.6",
    "css-loader": "^1.0.1",
    "dotenv": "^6.1.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "json-loader": "^0.5.7",
    "postcss-loader": "^3.0.0",
    "react-router-test-context": "^0.1.0",
    "react-test-renderer": "^16.6.3",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "style-loader": "^0.23.1",
    "stylelint": "^9.0.0",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-recommended": "^2.1.0",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "modulePaths": [
      "src"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(module\\.css)$": "identity-obj-proxy",
      "\\.(css)$": "<rootDir>/src/__mocks__/styleMock.js"
    },
    "coverageDirectory": "src/__coverage__",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/__coverage__/**"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "testURL": "http://localhost:8080/"
  }
}
